/**
 * 
 */
package main;

import conjunto.Conjunto;

/**
 * @author garciparedes
 *
 */
public class Main {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Conjunto<Integer> conjuntoA = new Conjunto();
		
		conjuntoA.agrega(1);
		conjuntoA.agrega(2);
		conjuntoA.agrega(3);

		System.out.println(conjuntoA.toString());
		
		conjuntoA.agrega(1);
		conjuntoA.elimina(2);
		conjuntoA.agrega(3);
		
		System.out.println(conjuntoA.toString());
		
		System.out.println(conjuntoA.esta(2));
		System.out.println(conjuntoA.esta(3));


		Conjunto<Integer> conjuntoB = new Conjunto(2,3,4,5,6);

		System.out.println(conjuntoB.toString());

		System.out.println(conjuntoA.union(conjuntoB).toString());
	}

}
