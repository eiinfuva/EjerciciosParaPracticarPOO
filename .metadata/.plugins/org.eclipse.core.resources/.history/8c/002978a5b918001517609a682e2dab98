/**
 * 
 */
package main;

import conjunto.Conjunto;

/**
 * @author garciparedes
 *
 */
public class Main {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Conjunto<Integer> conjuntoA = new Conjunto<Integer>();
		
		System.out.println(conjuntoA);

		conjuntoA.agrega(1);
		conjuntoA.agrega(2);
		conjuntoA.agrega(3);

		System.out.println(conjuntoA);
		
		conjuntoA.agrega(1);
		conjuntoA.elimina(2);
		conjuntoA.agrega(3);
		
		System.out.println(conjuntoA);
		

		Conjunto<Integer> conjuntoB = new Conjunto<Integer>(2,3,4,5,6);

		System.out.println(conjuntoB);
		
		System.out.println(conjuntoA.esta(2));
		System.out.println(conjuntoA.esta(3));
		

		Conjunto<Integer> conjuntoUnion = conjuntoA.union(conjuntoB);
		System.out.println(conjuntoUnion);

		
		Conjunto<Integer> conjuntoInterseccion = conjuntoA.interseccion(conjuntoB);
		System.out.println(conjuntoInterseccion);
	}

}
